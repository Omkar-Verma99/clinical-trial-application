{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "out",
    "headers": [
      {
        "source": "/sw.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate, max-age=0"
          },
          {
            "key": "Pragma",
            "value": "no-cache"
          }
        ]
      },
      {
        "source": "/version.json",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate, max-age=0"
          }
        ]
      },
      {
        "source": "/**/*.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/**/*.css",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  },
  "apphosting": {
    "backendId": "app",
    "rootDir": ".",
    "ignore": [
      "node_modules",
      ".next",
      ".git",
      ".gitignore",
      "*.log",
      ".DS_Store",
      ".env.local",
      ".vercel",
      ".venv",
      ".vscode",
      ".zencoder",
      ".zenflow"
    ],
    "backends": [
      {
        "codebase": "default",
        "backendId": "app",
        "location": "us-central1"
      }
    ]
  }
}